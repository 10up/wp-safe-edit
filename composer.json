{
    "name": "wp-post-forking",
    "description": "A WordPress plugin that enables the ability to edit a published post behind the scene, and publish the changes when ready.",
    "version": "0.1.0",
    "type": "wordpress-plugin",
    "keywords": [],
    "homepage": "https://github.com/10up/post-forking",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Phillips",
            "email": "michael.phillips@10up.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "10up/wp_mock": "0.2.0"
    },
    "autoload": {
        "psr-4": {
            "TenUp\\PostForking\\": "includes"
        },
        "files": [
            "includes/functions/helpers.php",
            "includes/functions/db-helpers.php",
            "includes/functions/post-helpers.php",
            "includes/functions/log-helpers.php"
        ]
    }
}
